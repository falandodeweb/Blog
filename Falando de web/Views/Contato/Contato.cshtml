@Html.Raw(ViewBag.Texto)

<form id="form-contato" method="post" action="@Request.Url.AbsolutePath">
    <div class="row control-group">
        <div class="form-group col-xs-12 floating-label-form-group controls">
            <label>Nome</label>
            <input type="text" class="form-control" placeholder="Nome" id="txt_nome" name="txt_nome" required>
        </div>
    </div>
    <div class="row control-group">
        <div class="form-group col-xs-12 floating-label-form-group controls">
            <label>E-mail</label>
            <input type="email" class="form-control" placeholder="E-mail" id="txt_email" name="txt_email" required>
        </div>
    </div>
    <div class="row control-group">
        <div class="form-group col-xs-12 floating-label-form-group controls">
            <label>Telefone</label>
            <input type="tel" class="form-control" placeholder="Telefone" id="txt_telefone" name="txt_telefone" required>
        </div>
    </div>
    <div class="row control-group">
        <div class="form-group col-xs-12 floating-label-form-group controls">
            <label>Mensagem</label>
            <textarea rows="5" class="form-control" placeholder="Mensagem" id="txt_msg" name="txt_msg" required></textarea>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="form-group col-xs-12">
            <button type="submit" class="btn btn-default">Enviar!</button>
        </div>
    </div>
</form>